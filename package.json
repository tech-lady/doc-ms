{
  "name": "document-api",
  "version": "1.0.0",
  "description": "The system manages documents, users and user roles.",
  "main": "server.js",
  "scripts": {
    "start": "node_modules/.bin/babel-node server.js",
    "start:dev": "nodemon --exec node_modules/.bin/babel-node server.js",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "test": "cross-env npm run coverage",
    "coverage": "cross-env npm run 'drop:db' && npm run 'create:db' && npm run 'migrate:test' && NODE_ENV=test node_modules/.bin/istanbul cover  ./node_modules/.bin/_mocha  ./server/tests/**/*.js -- --require babel-register && cat ./coverage/lcov.info | node_modules/.bin/coveralls",
    "seed": "NODE_ENV=test sequelize db:seed:all",
    "drop:db": "psql -c 'drop database if exists doc_api_test;' -U postgres",
    "create:db": "psql -c 'create database doc_api_test;' -U postgres",
    "migrate:test": "NODE_ENV=test sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-oawofadeju/document-api.git"
  },
